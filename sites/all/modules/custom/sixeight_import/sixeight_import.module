/**
 * Implements hook_cron_queue_info().
 */
function sixeight_import_cron_queue_info() {
  $queues['import_sermon_series'] = array(
    'worker callback' => '_import_sermon_series',
    'time' => 60,
  );
  $queues['import_sermons'] = array(
    'worker callback' => '_import_sermons',
    'time' => 60,
  );
  $queues['import_blogs'] = array(
    'worker callback' => '_import_blogs',
    'time' => 60,
  );
  return $queues;
}

/**
 * Implements hook_cron().
 */
function sixeight_import_cron() {
  $stage = variable_get('sixeight_import_stage', 0);

  for ($i=1; $i<=10; $i++) {
    switch ($stage) {
    case 1:
      $queue = DrupalQueue::get('import_sermon_series');
      $item = $queue->claimItem();

      if (is_object($item)) {
        _import_sermon_series($item->data);
        $queue->deleteItem($item);
      }
      else {
        variable_set('sixeight_import_stage', $stage+1);
      }

      break;
    case 2:
      $queue = DrupalQueue::get('import_sermons');
      $item = $queue->claimItem();

      if (is_object($item)) {
        _import_sermons($item->data);
        $queue->deleteItem($item);
      }
      else {
        variable_set('sixeight_import_stage', $stage+1);
      }

      break;
    case 3:
      $queue = DrupalQueue::get('import_blogs');
      $item = $queue->claimItem();

      if (is_object($item)) {
        _import_blogs($item->data);
        $queue->deleteItem($item);
      }
      else {
        variable_set('sixeight_import_stage', $stage+1);
      }

      break;
    }
  }
}









function _import_sermon_series($data) {
  $sermon_series_map = variable_get('sixeight_import_sermon_series_map', array());
  $users_array = variable_get('sixeight_import_user_map', array());

  $s = $data['sermon_series'];

  $new_ss = new stdClass();
  $new_ss->title = $s['title'];
  $new_ss->created = $s['created'];
  $new_ss->status = 1;
  $new_ss->uid = $users_array[$s['uid']];
  $new_ss->comment = 0;
  $new_ss->promote = 0;
  $new_ss->sticky = 0;
  $new_ss->language = LANGUAGE_NONE;
  $new_ss->type = 'sermon_series';
  $new_ss->field_series_description[LANGUAGE_NONE][0]['value'] = $s['body'];

  node_object_prepare($new_ss);
  node_save($new_ss);

  $new_ss->created = $s['created'];
  node_save($new_ss);

  $sermon_series_map[$s['nid']] = $new_ss->nid;

  variable_set('sixeight_import_sermon_series_map', $sermon_series_map);
}

